[alias]
  graph = log --graph --date-order -C -M --pretty=format:\"<%h> %ad [%cn] %Cgreen%d%Creset %s\" --all --date=iso

[user]
  name = grohiro
  email = khirokawa@growk.jp
[http]
	sslVerify = true
  receivepack = true 
  postBuffer = 524288000
[https]
[color]
  ui = auto
[core]
  excludesfile = ~/.gitignore
  whitespace = cr-at-eol
[difftool "sourcetree"]
  cmd = opendiff \"$LOCAL\" \"$REMOTE\"
  path = 
[diff]
  tool = vimdiff
[credential]
[credential]
  helper = osxkeychain
[push]
  default = simple
[pager]
  log = diff-highlight | less
  show = diff-highlight | less
  diff = diff-highlight | less
